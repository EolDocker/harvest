LoadModule php5_module /usr/lib/apache2/modules/libphp5.so
<VirtualHost *:80>
  ServerAdmin webmaster@localhost
  DocumentRoot /var/www/eol_php_code/applications
  ServerName services.eol.org

  <Directory /var/www/eol_php_code/applications>
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  ErrorLog /var/log/apache2/error.log
  CustomLog /var/log/apache2/access.log combined

</VirtualHost>

# vim: syntax=apache
